<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_237"
    android:background="@drawable/yuan"
    android:orientation="vertical"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_21"
        android:text="时间选择"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_15"
        />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        >

        <TextView
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_20"
            android:text="开始时间:"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            android:layout_marginTop="@dimen/dp_25"
            />
        <Button
            android:id="@+id/start_time"
            android:layout_width="@dimen/dp_135"
            android:layout_height="@dimen/dp_29"
            android:background="#FFF2F2F2"
            android:layout_toRightOf="@+id/time"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="2019-03-05"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/line"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_20"
            android:text="-"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            android:layout_toRightOf="@+id/start_time"
            android:layout_marginTop="@dimen/dp_25"
            android:layout_marginLeft="@dimen/dp_5"
            />
        <Button
            android:id="@+id/start_times"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_29"
            android:background="#FFF2F2F2"
            android:layout_toRightOf="@+id/line"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="00:00:00"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        >

        <TextView
            android:id="@+id/times"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_20"
            android:text="结束时间:"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            android:layout_marginTop="@dimen/dp_25"
            />
        <Button
            android:id="@+id/end_time"
            android:layout_width="@dimen/dp_135"
            android:layout_height="@dimen/dp_29"
            android:background="#FFF2F2F2"
            android:layout_toRightOf="@+id/times"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="2019-03-05"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/lines"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_20"
            android:text="-"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            android:layout_toRightOf="@+id/end_time"
            android:layout_marginTop="@dimen/dp_25"
            android:layout_marginLeft="@dimen/dp_5"
            />
        <Button
            android:id="@+id/end_times"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_29"
            android:background="#FFF2F2F2"
            android:layout_toRightOf="@+id/lines"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="00:00:00"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_60"
        >

        <Button
            android:id="@+id/quxaio"
            android:layout_width="@dimen/dp_167"
            android:layout_height="@dimen/dp_33"
            android:layout_marginLeft="@dimen/dp_15"
            android:background="@drawable/sel_but11"
            android:text="取消"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_16"
            />
        <Button
            android:layout_width="@dimen/dp_167"
            android:layout_height="@dimen/dp_33"
            android:layout_marginLeft="@dimen/dp_11"
            android:background="@drawable/sel_but4"
            android:text="确定"
            android:textColor="#ff333333"
            android:layout_toRightOf="@+id/quxaio"
            android:textSize="@dimen/sp_16"
            />

    </RelativeLayout>
</LinearLayout>
